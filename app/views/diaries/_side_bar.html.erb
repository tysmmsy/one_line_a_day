<div class="side-bar-header">
  <div class="search-input">
    <input class="search-diary" placeholder= "search word">
  </div>
  <input type="submit" value="&#xf002;" class="fas">
</div>

<hr width="200">

<div class="contents">
  <% @diaries.each do |diary| %>
    <div class="content">
      <div class="content-date">
        <%= diary.date%>
      </div>
      <div class="content-name">
        <%= diary.content%>
      </div>
    </div>
  <% end %>
</div>